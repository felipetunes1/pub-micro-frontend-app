  <single-spa-router id="single-spa-layout">
    <header></header>
    <nav></nav>
    <main></main>
    <footer></footer>
  </single-spa-router>

  <style>
    main {
      min-height: 475px;
      padding: 20px;
    }

    footer {
      right: 0;
      bottom: 0;
      margin-bottom: 5px;
    }

    .CodeMirror {
   border: 1px solid #eee;
}

ngx-codemirror .CodeMirror,
ngx-codemirror .CodeMirror-scroll {
   height:500px;
   max-height:500px;
}

.routeGenericRemove {
   background-color: red;
   color: white;
}

.routeGenericEdit {
   background-color: blue;
   color: white;
}

.routeGenericNew {
   background-color: green;
   color: white;
   
}

.routeGeneric {
   margin: 5px 0px;
   cursor: pointer;
}

.routeGeneric ::-webkit-scrollbar-track {
   background-color: #F4F4F4;
 }
 .routeGeneric ::-webkit-scrollbar {
   width: 6px;
   height: 6px;
   background: #F4F4F4;
 }
 .routeGeneric ::-webkit-scrollbar-thumb {
   background: #dad7d7;
 }

 .routeGeneric .mat-container {
   scrollbar-color: #dad7d7 #F4F4F4;
   scrollbar-width: 6px;
 }
</style>
</head>

<body>
  <script>
    System.import('@hdi-seguros/root-config');
  </script>
  <import-map-overrides-full show-when-local-storage="devtools" dev-libs></import-map-overrides-full>
